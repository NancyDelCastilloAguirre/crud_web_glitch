<%- include("_header") -%>

<h1>Introduce los datos: </h1>

  
<div class="table-responsive-sm">
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Nombre</th>
        <th>Autor</th>
        <th>Descripci√≥n</th>
        <th class="d-print-none">
          <a class="btn btn-sm btn-success" href="/create">Agregar</a>
        </th>
      </tr>
    </thead>
    <tbody>
      <% for (const book of model) { %>
        <tr>
          <td><%= book.Titre %></td>
          <td><%= book.Auteur %></td>
          <td><%= book.Commentaires %></td>
          <td class="d-print-none">
            <a class="btn btn-sm btn-warning" href="/edit/<%= book.Livre_ID %>">Modificar</a>
            <a class="btn btn-sm btn-danger" href="/delete/<%= book.Livre_ID %>">Eliminar</a>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>
</div>


